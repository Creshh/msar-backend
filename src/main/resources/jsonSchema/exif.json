{
	"$id": "https://tu-chemnitz.de/tomkr.meta.exif",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "EXIF",
	"description": "EXIF image metadata",
	"type": "object",
	"extends": {
		"$ref": "metaData.json"
	},
	"properties": {
		"xdim": {
			"type": "integer",
			"required": true
		},
		"ydim": {
			"type": "integer",
			"required": true
		},
		"datetime": {
			"type": "string",
			"required": true
		},
		"model": {
			"type": "string"
		},
		"lens": {
			"type": "string"
		},
		"orientation": {
			"type": "string"
		},
		"datetimeoriginal": {
			"type": "string"
		},
		"compression": {
			"type": "string"
		},
		"fnumber": {
			"type": "string"
		},
		"exposuretime": {
			"type": "string"
		},
		"isospeed": {
			"type": "string"
		},
		"meteringmode": {
			"type": "string",
			"required": true
		},
		"focallength": {
			"type": "string",
			"required": true
		},
		"flash": {
			"type": "string"
		}
	}
}